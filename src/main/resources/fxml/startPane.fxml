<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.facebook.maciejprogramuje.Controllers.StartPaneController">
   <Label alignment="CENTER" text="Start new game?">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </VBox.margin>
   </Label>
   <HBox alignment="CENTER" spacing="10.0">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </VBox.margin>
      <Button fx:id="yesButton" mnemonicParsing="false" text="Yes">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
      </Button>
      <Button fx:id="noButton" mnemonicParsing="false" text="No"/>
   </HBox>
</VBox>
